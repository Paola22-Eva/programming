

def santa_user(p_list):
    dictionary = dict() 
    lenght = len(p_list)
    for n in range(lenght):
        if len(p_list[n]) > 1:
            dictionary[p_list[n][0]] = p_list[n][1]
        else:
            dictionary[p_list[n][0]] = None
    return dictionary


def  main():

    n = int(input("Введите кол-во пользователей: "))    
    parent_list = [] 
    for i in range(n):
        user = [x for x in input().split()] 
        user[0] = user[0] + " " + user[1]
        user.pop(1)
        parent_list.append(user) 

    dictionary = santa_user(parent_list)
    print(dictionary)
if __name__ == "__main__":
    main()
def main():
  N=int(input())
  spisok=[int(input()) for _ in range(N)]
  import itertools
  spis=itertools.permutations(spisok, 4)
  def summa(spis1):
    ras, otv=10000, []
    C, bC=int(input()), 0
    for s in spis1:
      if sum(list(s))==C:
        otv=list(s)
        print(otv, C)
        break
      if sum(list(s))<0:
        rasse=C-sum(list(s))
        if rasse <0:
          rasse=-rasse
        if rasse<ras:
          ras=rasse
          otv=list(s)
          bC=sum(list(s))
      if sum(list(s))>0:
        if sum(list(s))-C<ras:
          ras=sum(list(s))-C
          otv=list(s)
          bC=sum(list(s))
    print(otv)
    print(bC)
          
  return(summa(spis))


  
if __name__=="__main__":
  main()


def main():
  
  vvod=list(input())
  otvet=set()
  spis=[]
  for sim in vvod:
    if (sim!="{" and sim!="}" and sim!=" " and sim!="," and sim not in spis):
      spis.append(sim)

  from itertools import combinations
  for nom in range(1, len(spis)+1):
    podmnozh=set(combinations(spis, nom))
    otvet.update(podmnozh)

  print("Подмножества: ", (otvet))
  print("Кол.-во подмножеств:", len(otvet))
    
if __name__=="__main__":
  main()


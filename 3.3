def main():
    spisok=input()
    otvet=[]
    while spisok!=[]:
        promspisok=[]
        slovo=spisok.pop(0)
        promspisok.append(slovo)
        for nom in range(len(spisok)-1):
          if len(slovo)==len(spisok[nom]):
            chet=0
            for byk in slovo:
              if byk in spisok[nom]:
                chet+=1
            if chet==len(slovo):  
              promspisok.append(spisok.pop(nom))
        otvet.append(promspisok)

    print(otvet)
    
if __name__=="__main__":
    main()

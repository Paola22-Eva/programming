stroka=input()
maxstroka= ""
promstroka=""
for nom1 in range(len(stroka)):
  for nom2 in range(nom1, len(stroka)):
    if stroka[nom2] in promstroka:
      break
    else:
      promstroka+=stroka[nom2]
  if len(promstroka)>len(maxstroka):
    maxstroka= promstroka
  promstroka= ""

print(maxstroka)
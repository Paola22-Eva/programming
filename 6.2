def Calculations(rimNum, dict_of_nums, ArabNum = 0):
    keys = list(dict_of_nums.keys()) 
    values = list(dict_of_nums.values()) 
    for i in range(len(keys)): 
        cnt_of_exception = rimNum.count(keys[i]) 
        rimNum = rimNum.replace(keys[i], "", cnt_of_exception) 
        ArabNum += values[i] * cnt_of_exception #
    return (ArabNum, rimNum) 

def Conversion(rimNum):
    exceptions = {          
        "IX": 9,
        "IV": 4,
        "XL": 40,
        "XC": 90,
        "CD": 400,
        "CM": 900,} 

    numbers = {            
        "I": 1,
        "V": 5,
        "X": 10,
        "L": 50,
        "C": 100,
        "D": 500,
        "M": 1000,
    }

    ArabNum = 0
    ArabNum, rimNum = Calculations(rimNum, exceptions) 
    ArabNum, rimNum = Calculations(rimNum, numbers, ArabNum) 

    return ArabNum
    


def  main():
    rimNum = input() 
    arabNum = Conversion(rimNum)
    print(arabNum)

if __name__ == "__main__":
    main()